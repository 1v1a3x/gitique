{
  "manifest_version": 2,
  "name": "Gitique (development mode)",
  "description": "Make GitHub code review better",
  "version": "0.1",
  "version_name": "development",
  "permissions": [
    "https://github.com/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
